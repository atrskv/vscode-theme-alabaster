{
    "$schema": "vscode://schemas/color-theme",
    "name": "Alabaster",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#695900"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.regexp",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#064200"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#7A3E9D"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character"
            ],
            "settings": {
                "foreground": "#7A3E9D"
            }
        },
        {
            "name": "Global definitions",
            "scope": "entity.name",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Function Definitions",
            "scope": [
                "entity.name.function",
                "entity.name.function.magic",
                "support.function.magic"
            ],
            "settings": {
                "foreground": "#003E7E"
            }
        },
         {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "entity.name.function.decorator",
                "meta.function.decorator"
            ],
            "settings": {
                "foreground": "#666666"
            }
        },
        {
            "name": "Decorator Arguments",
            "scope": [
                "meta.function.decorator.python meta.function-call.arguments"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#999999"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#FFCCCC",
                "foreground": "#CC3333"
            }
        },
        {
            "name": "Extra: Diff Range",
            "scope": [
                "meta.diff.range",
                "meta.diff.index",
                "meta.separator"
            ],
            "settings": {
                "background": "#DDDDFF",
                "foreground": "#434343"
            }
        },
        {
            "name": "Extra: Diff From",
            "scope": "meta.diff.header.from-file",
            "settings": {
                "background": "#FFDDDD",
                "foreground": "#434343"
            }
        },
        {
            "name": "Extra: Diff To",
            "scope": "meta.diff.header.to-file",
            "settings": {
                "background": "#DDFFDD",
                "foreground": "#434343"
            }
        }
    ],
    "colors": {
        "editor.background": "#FFFFFF",
        "editor.foreground": "#000",
        "editor.lineHighlightBackground": "#00000000",
        "editor.lineHighlightBorder": "#00000000",
        "editor.selectionBackground": "#B4D8FD",
        "editor.selectionHighlightBackground": "#00000000",
        "editor.wordHighlightBackground": "#EFEFEF",
        "editorGroupHeader.tabsBackground": "#F7F8FA",
        "editorLineNumber.foreground": "#999999",
        "editorLineNumber.activeForeground": "#585858",
        "editorCursor.foreground": "#007ACC",
        "editorCursor.background": "#FFFFFF",
        "editor.findMatchBackground": "#B4D8FD",
        "editor.findMatchHighlightBackground": "#FFBC5D",
        "editor.stackFrameHighlightBackground": "#B4D8FD",
        "editorBracketHighlight.foreground1": "#000000",
        "editorBracketHighlight.foreground2": "#000000",
        "editorBracketHighlight.foreground3": "#000000",
        "editorBracketHighlight.foreground4": "#000000",
        "editorBracketHighlight.foreground5": "#000000",
        "editorBracketHighlight.foreground6": "#000000",
        "editorBracketHighlight.unexpectedBracket.foreground": "#000000",
        "editorSuggestWidget.background": "#F6F6F6",
        "editorSuggestWidget.selectedBackground": "#D5E1FF",
        "editorSuggestWidget.focusHighlightForeground": "#000000",
        "editorSuggestWidget.highlightForeground": "#000000",
        "editorSuggestWidget.selectedForeground": "#000000",
        "panel.background": "#F7F8FA",
        "terminal.background": "#FFFFFF",
        "button.background": "#585858",
        "activityBarBadge.background": "#585858",
        "list.highlightForeground": "#585858",
        "list.foreground": "#000000",
        "list.inactiveForeground": "#000000",
        "list.inactiveFocusForeground": "#000000",
        "list.activeSelectionBackground": "#D5E1FF",
        "list.activeSelectionForeground": "#000000",
        "list.inactiveSelectionBackground": "#DEE1E5",
        "list.inactiveSelectionForeground": "#000000",
        "list.focusHighlightForeground": "#000000",
        "sideBarSectionHeader.background": "#00000000",
        "sideBar.background": "#F7F8FA",
        "sideBar.foreground": "#000000",
        "activityBar.border": "#EFEFEF",
        "sideBar.border": "#EFEFEF",
        "statusBar.border": "#EFEFEF",
        "editorLink.activeForeground": "#007ACC",
        "activityBar.background": "#F7F8FA",
        "activityBar.foreground": "#585858",
        "statusBar.background": "#F7F8FA",
        "statusBar.foreground": "#585858",
        "statusBar.debuggingBackground": "#F7F8FA",
        "statusBar.debuggingForeground": "#000",
        "statusBar.noFolderBackground": "#F7F8FA",
        "statusBar.noFolderForeground": "#585858",
        "focusBorder": "#00000000",
        "quickInputList.focusForeground": "#000000",
        "quickInputList.focusBackground": "#D5E1FF",
        "titleBar.activeBackground": "#27282D",
        "titleBar.activeForeground": "#F7F8FA",
        "tab.inactiveBackground": "#DEE1E5",
        "terminal.ansiWhite": "#C9C9C9",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#063FDB",
        "terminal.ansiCyan": "#028E8E",
        "terminal.ansiGreen": "#067D17",
        "terminal.ansiMagenta": "#B309B3",
        "terminal.ansiRed": "#CE0505",
        "terminal.ansiYellow": "#B28C00",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBrightBlack": "#555555",
        "terminal.ansiBrightBlue": "#2D61F0",
        "terminal.ansiBrightCyan": "#15C1C1",
        "terminal.ansiBrightGreen": "#16B42C",
        "terminal.ansiBrightMagenta": "#E617E6",
        "terminal.ansiBrightRed": "#FF1616",
        "terminal.ansiBrightYellow": "#ECC32C"
    }
}
